	 -->
	
	<!-- 日程管理开始 -->
	<url id="syseveschedule001" path="/post/ScheduleDayController/insertScheduleDayMation" val="添加日程信息" allUse="2">
		<property id="scheduleTitle" name="scheduleTitle" ref="required" var="日程标题"/>
		<property id="remarks" name="remarks" ref="" var="备注"/>
		<property id="allDay" name="allDay" ref="required,num" var="是否全天"/>
		<property id="scheduleStartTime" name="scheduleStartTime" ref="required" var="开始日期"/>
		<property id="scheduleEndTime" name="scheduleEndTime" ref="required" var="结束日期"/>
		<property id="remindType" name="remindType" ref="required,num" var="提醒时间所属类型  1.日程开始时 2.5分钟前 3.15分钟前 4.30分钟前 5.1小时前 6.2小时前 7.1天前 8.2天前 9.一周前"/>
		<property id="type" name="type" ref="required,num" var="日程类型 1个人 2工作 3节假日 4生日 5自定义"/>
		<property id="typeName" name="typeName" ref="required" var="类型名称"/>
		<property id="import" name="import" ref="required,num" var="日程重要性  1普通  2重要"/>
	</url>
	<url id="syseveschedule002" path="/post/ScheduleDayController/queryScheduleDayMationByUserId" val="根据用户日程信息" allUse="2">
