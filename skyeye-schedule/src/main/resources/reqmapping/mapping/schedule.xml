	</url>
	<url id="syseveschedule009" path="/post/ScheduleDayController/downloadScheduleTemplate" val="下载节假日导入模板" allUse="1">
	</url>
	<url id="syseveschedule010" path="/post/ScheduleDayController/exploreScheduleTemplate" val="导入节假日日程" allUse="1">
	</url>
