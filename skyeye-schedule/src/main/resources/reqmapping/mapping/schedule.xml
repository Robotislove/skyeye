		<property id="startTime" name="startTime" ref="required" var="开始时间"/>
		<property id="endTime" name="endTime" ref="required" var="结束时间"/>
	</url>
	<url id="syseveschedule017" path="/post/ScheduleDayController/addSchedule" val="新增节假日" allUse="1">
		<property id="title" name="title" ref="required" var="节假日标题"/>
		<property id="startTime" name="startTime" ref="required" var="开始时间"/>
		<property id="endTime" name="endTime" ref="required" var="结束时间"/>
	</url>
	<url id="syseveschedule018" path="/post/ScheduleDayController/queryHolidayScheduleListBySys" val="获取所有节假日" allUse="2">
