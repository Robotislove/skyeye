		<property id="parentId" name="parentId" ref="required" var="父id"/>
	</url>
	<url id="dsformpagetype006" path="/post/DsFormPageTypeController/queryDsFormPageTypeByParentId" method="GET" val="通过parentId查找对应的动态表单分类列表" allUse="2" groupName="动态表单页面分类模块">
		<property id="parentId" name="parentId" ref="required" var="父id"/>
	</url>
	<!-- 动态表单页面分类结束 -->

</controller>