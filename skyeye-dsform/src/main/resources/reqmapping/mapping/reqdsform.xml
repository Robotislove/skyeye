		<property id="require" name="require" ref="" var="表单项限制条件"/>
		<property id="placeholder" name="placeholder" ref="" var="文本提示语" />
		<property id="defaultValue" name="value" ref="" var="默认值"/>
		<property id="formContentId" name="formContentId" ref="required" var="展现形式id" />
		<property id="htmlContent" name="htmlContent" ref="required" var="HTML文本"/>
		<property id="jsContent" name="jsContent" ref="" var="JS文本"/>
		<property id="associatedDataTypes" name="associatedDataTypes" ref="" var="关联数据类型"/>
		<property id="aData" name="aData" ref="" var="数据"/>
		<property id="keyId" name="keyId" ref="" var="控件Id"/>
		<property id="editableNodeId" name="editableNodeId" ref="" var="可编辑节点Id"/>
		<property id="editableNodeName" name="editableNodeName" ref="" var="可编辑节点名称"/>
	</url>
	<url id="dsformpage004" path="/post/DsFormPageController/selectFormPageContentByPageId" val="查看某个动态表单中的表单控件" allUse="1" groupName="表单页面设计接口">
		<property id="rowId" name="pageId" ref="required" var="页面id" />
	</url>
	<url id="dsformpage005" path="/post/DsFormPageController/deleteDsFormPageById" val="删除动态表单页面" method="DELETE" allUse="1" groupName="表单页面设计接口">
		<property id="rowId" name="pageId" ref="required" var="页面id" />
	</url>
	<url id="dsformpage006" path="/post/DsFormPageController/selectDsFormPageById" val="通过id查找对应的动态表单页面" method="GET" allUse="2" groupName="表单页面设计接口">
		<property id="rowId" name="id" ref="required" var="页面id" />
	</url>
	<url id="dsformpage007" path="/post/DsFormPageController/editDsFormPageMationById" val="通过id编辑对应的动态表单页面" method="PUT" allUse="1" groupName="表单页面设计接口">
		<property id="rowId" name="id" ref="required" var="页面id" />
		<property id="pageName" name="pageName" ref="" var="页面名称" />
		<property id="pageDesc" name="pageDesc" ref="" var="页面简介" />
		<property id="firstTypeId" name="firstTypeId" ref="required" var="所属一级分类" />
		<property id="secondTypeId" name="secondTypeId" ref="required" var="所属二级分类" />
	</url>
	<url id="dsformpage008" path="/post/DsFormPageController/queryAllDsFormPageContent" val="获取表单内所有控件用于编辑调整" allUse="1" groupName="表单页面设计接口">
		<property id="rowId" name="id" ref="required" var="页面id" />
	</url>
	<url id="dsformpage009" path="/post/DsFormPageController/editDsFormPageContentByPageId" val="编辑表单内容中的控件" allUse="1" groupName="表单页面设计接口">
		<property id="formedit" name="formedit" ref="" var="数据" />
	</url>
	<url id="dsformpage010" path="/post/DsFormPageController/queryInterfaceIsTrueOrNot" val="验证接口是否正确" allUse="2" groupName="表单页面设计接口">
		<property id="interfa" name="interfa" ref="" var="数据" />
	</url>
	<url id="dsformpage011" path="/post/DsFormPageController/queryInterfaceValue" val="获取接口中的值" allUse="2" groupName="表单页面设计接口">
		<property id="interfa" name="interfa" ref="" var="数据" />
	</url>
	<url id="dsformpage012" path="/post/DsFormPageController/queryDsFormContentListByPageId" val="获取redis中的动态表单页" allUse="2" groupName="表单页面设计接口">
		<property id="rowId" name="pageId" ref="required" var="页面id" />
	</url>
	<url id="dsformpage013" path="/post/DsFormPageController/queryDsFormContentListByCode" val="根据code获取动态表单信息" allUse="2" method="GET" groupName="表单页面设计接口">
		<property id="dsFormCode" name="dsFormCode" ref="required" var="com.skyeye.common.constans.SystemFoundationSettingsConstants.CustomWithDsFormObject中的code" />
	</url>
	<url id="dsformpage014" path="/post/DsFormPageController/saveDsFormDataList" val="保存动态表单信息" allUse="2" method="POST" groupName="表单页面设计接口">
		<property id="dataJson" name="dataJson" ref="required,json" var="数据" />
		<property id="objectId" name="objectId" ref="required" var="需要绑定的id" />
	</url>
	<url id="dsformpage015" path="/post/DsFormPageController/queryDsFormDataListByObjectId" val="根据objectId获取动态表单信息" allUse="2" method="GET" groupName="表单页面设计接口">
		<property id="objectId" name="objectId" ref="required" var="绑定的id" />
	</url>
