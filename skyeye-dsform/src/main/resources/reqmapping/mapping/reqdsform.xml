	<url id="dsformpagetype002" path="/post/DsFormPageTypeController/insertDsFormPageType" method="POST" val="新增动态表单页面分类" allUse="1" groupName="动态表单页面分类模块">
		<property id="typeName" name="typeName" ref="required" var="分类名称"/>
		<property id="parentId" name="parentId" ref="required" var="父id"/>
	</url>
	<url id="dsformpagetype003" path="/post/DsFormPageTypeController/delDsFormPageTypeById" method="DELETE" val="删除动态表单分类" allUse="1" groupName="动态表单页面分类模块">
		<property id="id" name="id" ref="required" var="动态表单分类id"/>
	</url>
	<url id="dsformpagetype004" path="/post/DsFormPageTypeController/queryDsFormPageTypeById" method="GET" val="根据id查询动态表单页面分类详情" allUse="2" groupName="动态表单页面分类模块">
		<property id="id" name="id" ref="required" var="动态表单内容id"/>
	</url>
