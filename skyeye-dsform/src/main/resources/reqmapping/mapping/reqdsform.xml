	<!-- 动态表单页面设计开始 -->
	<url id="dsformpage001" path="/post/DsFormPageController/queryDsFormPageList" val="获取动态表单页面表" method="POST" allUse="2" groupName="表单页面设计接口">
        <property id="limit" name="limit" ref="required,num" var="分页参数,每页多少条数据" />
        <property id="page" name="page" ref="required,num" var="分页参数,第几页"/>
        <property id="pageName" name="pageName" ref="" var="页面名称"/>
		<property id="firstTypeId" name="firstTypeId" ref="" var="所属一级分类" />
		<property id="secondTypeId" name="secondTypeId" ref="" var="所属二级分类" />
    </url>
    <url id="dsformpage002" path="/post/DsFormPageController/insertDsFormPageMation" val="新增动态表单页面" method="POST" allUse="1" groupName="表单页面设计接口">
        <property id="pageName" name="pageName" ref="required" var="页面名称" />
        <property id="pageDesc" name="pageDesc" ref="" var="页面简介"/>
		<property id="firstTypeId" name="firstTypeId" ref="required" var="所属一级分类" />
		<property id="secondTypeId" name="secondTypeId" ref="required" var="所属二级分类" />
    </url>
	<url id="dsformpage003" path="/post/DsFormPageController/insertDsFormPageContent" val="新增控件到表单页面" allUse="2" groupName="表单页面设计接口">
