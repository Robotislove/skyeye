<?xml version="1.0" encoding="UTF-8"?>

<controller>
	
	<!-- 手机端学生登录开始 -->
	<url id="schooluser001" path="/post/SchoolUserController/queryStuMationToLogin" val="手机端学生登录" allUse="0">
		<property id="userCode" name="userCode" ref="required" var="账号"/>
		<property id="password" name="password" ref="required" var="密码"/>
		<property id="cId" name="cId" ref="" var="cId,用于手机端消息通知"/>
	</url>
	<url id="schooluser002" path="/post/SchoolUserController/queryStuUserMation" val="手机端从session中获取学生信息" allUse="2">
	</url>
	<url id="schooluser003" path="/post/SchoolUserController/queryStuExit" val="手机端注销登录" allUse="2">
	</url>
	<url id="schooluser004" path="/post/SchoolUserController/queryStuUserMationDetailById" val="获取学生信息详细信息-我的名片" allUse="2">
	</url>
	<url id="schooluser005" path="/post/SchoolUserController/editUserPassword" val="修改密码" allUse="2">
		<property id="newPassword" name="newPassword" ref="required" var="新密码"/>
		<property id="oldPassword" name="oldPassword" ref="required" var="旧密码"/>
	</url>
	<!-- 手机端学生登录结束 -->
	
	<!-- 学生试卷相关开始 -->
	<url id="schoolstuexam001" path="/post/SchoolStuExamController/queryMyWaitExamList" val="获取我的待考试试卷列表" allUse="2">
		<property id="limit" name="limit" ref="required,num" var="分页参数,每页多少条数据" />
		<property id="page" name="page" ref="required,num" var="分页参数,第几页"/>
	</url>
	<url id="schoolstuexam002" path="/post/SchoolStuExamController/queryMyEndExamList" val="获取我的已考试试卷列表" allUse="2">
		<property id="limit" name="limit" ref="required,num" var="分页参数,每页多少条数据" />
		<property id="page" name="page" ref="required,num" var="分页参数,第几页"/>
	</url>
	<!-- 学生试卷相关结束 -->
	
</controller>